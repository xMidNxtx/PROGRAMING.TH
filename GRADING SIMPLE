#include <iostream>
using namespace std;
int a,b,c;
int main() {
  int a,b,c;
  cin >> a >> b >> c;
  a += b;
  a += c;
  if (a < 50) cout << "F";
  else if (a<55) cout << "D";
  else if (a<60) cout << "D+";
  else if (a<65) cout << "C";
  else if (a<70) cout << "C+";
  else if (a<75) cout << "B";
  else if (a<80) cout << "B+";
  else cout << "A";
}
